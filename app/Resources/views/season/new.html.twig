{% extends 'admin.html.twig' %}

{% block content %}
<div class="container-fluid">
    <div class="row">
        <div class="col-xs-12">
            <h1 class="page-header">Season creation</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <input type="submit" value="Create" class="btn btn-success" />
            {{ form_end(form) }}
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <a href="{{ path('admin_season_index') }}" class="pull-right">Back to the list</a>
        </div>
    </div>
</div>
{% endblock %}
